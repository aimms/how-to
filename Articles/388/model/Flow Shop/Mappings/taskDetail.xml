<AimmsJSONMapping>
    <ObjectMapping>
        <ValueMapping name="errormessage"   maps-to="sp_taskStatusErrorMessage"/>
        <ValueMapping name="message"        maps-to="sp_taskStatusMessage"/>
        <ValueMapping name="id"             maps-to="sp_taskStatusID"/>
        <ValueMapping name="projectName"    maps-to="sp_taskStatusProjectName"/>
        <ValueMapping name="projectVersion" maps-to="sp_taskStatusProjectVersion"/>
        <ValueMapping name="queuetime"      maps-to="p_taskStatusQueueTime"/>
        <ValueMapping name="errorCode"      maps-to="p_taskStatusErrorCode"/>
        <ValueMapping name="returncode"     maps-to="p_taskStatusReturnCode"/>
        <ValueMapping name="runtime"        maps-to="p_taskStatusRuntime"/>
        <ValueMapping name="service"        maps-to="sp_taskStatusService"/>
        <ValueMapping name="starttime"      maps-to="sp_taskStartTime"/>
        <ValueMapping name="status"         maps-to="sp_taskStatus"/>
    </ObjectMapping>
</AimmsJSONMapping>
