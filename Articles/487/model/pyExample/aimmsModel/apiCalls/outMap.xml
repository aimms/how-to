<AimmsJSONMapping>
    <ObjectMapping>
        <ValueMapping name="numClusters" maps-to="pNumClusters"/>
        <ArrayMapping name="latitude">
            <ValueMapping iterative-binds-to="iLoc" maps-to="pLatitude(iLoc)" dense-write="pLatitude(iLoc)"/>
        </ArrayMapping>
        <ArrayMapping name="longitude">
            <ValueMapping iterative-binds-to="iLoc" maps-to="pLongitude(iLoc)" dense-write="pLongitude(iLoc)"/>
        </ArrayMapping>
    </ObjectMapping>
</AimmsJSONMapping>